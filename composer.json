{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:solarsoft0/nova.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "64robots/nova-fields": "^0.4.3",
        "algolia/algoliasearch-client-php": "^1.28",
        "barryvdh/laravel-cors": "^0.11.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/nova": "dev-master",
        "laravel/passport": "^7.0",
        "laravel/scout": "^6.0",
        "laravel/tinker": "^1.0",
        "mpociot/laravel-apidoc-generator": "^2.1",
        "nsavinov/nova-json-schema-field": "^1.0",
        "silvanite/nova-field-cloudinary": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled"
          
        ]
    },
 
    
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
